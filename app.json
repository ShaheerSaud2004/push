{
  "expo": {
    "name": "Khafī",
    "slug": "khafi",
    "version": "1.0.0",
    "newArchEnabled": true,
    "orientation": "portrait",
    "icon": "./appicon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.khafi.app",
      "buildNumber": "4",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./appicon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.khafi.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "name": "Khafī - The Islamic Hidden Word Game",
      "shortName": "Khafī",
      "lang": "en",
      "scope": "/",
      "themeColor": "#8B4513",
      "backgroundColor": "#F5E6D3",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/",
      "bundler": "metro",
      "description": "A fun party game for Muslims! Play the classic hidden word game with Islamic categories and find the imposter among your friends."
    },
    "scheme": "khafi",
    "plugins": [
      "expo-asset",
      "./plugins/with-game-center.js"
    ],
    "extra": {
      "eas": {
        "projectId": "614e2abe-c981-4a35-80c7-c20c4c41ae6d"
      }
    }
  }
}
